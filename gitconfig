[push]
  default = nothing
[color]
  ui = auto
[color "branch"]
  current = red
  local = yellow
  remote = green
[color "diff"]
  meta = cyan
  frag = magenta
  old = red
  new = green
  whitespace = red reverse
[color "status"]
  untracked = cyan
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  irebase = rebase -i --autosquash
  branchbydate = for-each-ref --sort=-committerdate refs/heads/
  wdiff = diff --word-diff=color
  wshow = show --word-diff=color
  authors = shortlog -sn
[core]
  excludesfile = ~/.gitignore
  trustctime = false
  editor = vim
[merge]
  tool = vimdiff
[include]
  path = .gitconfig.local
[diff]
  algorithm = patience
